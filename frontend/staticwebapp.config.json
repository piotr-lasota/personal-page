{
  "routes": [
    {
      "route": "/blog/register",
      "allowedRoles": ["owner"]
    },
    {
      "route": "/blog/*/comments",
      "allowedRoles": ["owner"]
    },
    {
      "route": "/login",
      "redirect": "/.auth/login/github"
    },
    {
      "route": "/logout",
      "redirect": "/.auth/logout"
    },
    {
      "route": "/.auth/login/twitter",
      "statusCode": 404
    },
    {
      "route": "/.auth/login/facebook",
      "statusCode": 404
    },
    {
      "route": "/.auth/login/aad",
      "statusCode": 404
    },
    {
      "route": "/.auth/login/google",
      "statusCode": 404
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/401"
    }
  }
}
